    @media (min-width: 768px) {
      .bd-placeholder-img-lg {
        font-size: 3.5rem;
      }
    }

    .nav-link {
      font-size: 20px;
      padding-right: 15px;
    }

    #contact {
      padding-right: 35px;
    }

    header {
      background-color: orangered;
    }

    body {
      /* background: url('/img/pawbg-removebg.png'); */
      position: relative;
      font-weight: 600;
    }

    body::before {
      content: "";
      /* background: url('/img/pawbg-removebg.png'); */
      position: absolute;
      height: 100%;
      width: 100%;
      opacity: 0.2;
      z-index: -2;
    }

    .col-md-7 {
      margin-left: 220px;
      background-color: white;
      height: 690px;
      
      border-style: solid;
      border-width: 2px;
      border-color: rgb(180, 178, 178);
      border-radius: 5px;
    }
    .needs-validation{
      padding-top: 10px;
    }
    /* .mb-3{
          padding: 20px;
        } */
   
    .btn {
     background: orangered;
      margin-top: 20px;
      color: white;
    }
    .btn1{
      margin-left: 536px;
    }
    .btn:hover {
      color: white;
    }
    .btn a{
      color: white;
      text-decoration: none;
    }
    .btn a:hover{
      color: white;
    }
    .py-5 img{
         height: 70px;
         width: 100px;
    }
    iframe{
        height: 200px;
        width: 845px;
    }
