                <!-- <form class="col-12 col-lg-auto mb-3 mb-lg-0 me-lg-3">
                <input type="search" class="form-control form-control-dark" placeholder="Search..." aria-label="Search">
            </form> -->
                <ul class="nav col-12 col-lg-auto ms-lg-auto mb-2 justify-content-center mb-md-0">
                    <li><a href="/dogs" class="nav-link  text-white" id="navlink">Dogs</a></li>
                    <li><a href="/cat" class="nav-link text-white" id="">Cats</a></li>
                    <li><a href="/" class="nav-link text-white" id="contact">Logout</a></li>
                    <!-- <li><a href="#" class="nav-link text-white" id="contact">Contact Us</a></li> -->
                    <!-- <li><a href="#" class="nav-link px-2 text-white"></a></li>
              <li><a href="#" class="nav-link px-2 text-white">FAQs</a></li>
              <li><a href="#" class="nav-link px-2 text-white">About</a></li> -->
                </ul>

                <!-- <div class="text-end">
              <button type="button" class="btn btn-outline-light me-lg-3 "><a href="" class="signup">Login</a></button>
              <button type="button" class="btn "><a href="" class=" signup">Signup</a></button>
            
            </div> -->
            </div>
        </div>
    </header>
    <main>

        <section class="py-5 text-center container">
            <div class="row py-lg-5">
                <div class="col-lg-6 col-md-8 mx-auto">
                    <img src="/img/dog.webp" alt="">
                    <p class="lead text-muted">“Such short little lives our pets have to spend with us, and they spend most of it waiting for us to come home each day.”</p>

                </div>
            </div>
        </section>

        
        </div>
        <div class="album py-5">
            <div class="container">
                <div class="row row-cols-1 row-cols-2 row-cols-md-3 g-3">
                    <% animals.forEach(function(items){%>
                        <div class="col">
                            <div class="card shadow-sm">
                                <img src="/uploads/<%=items.img%>" alt="">
                                <div class="card-body">
                                    <p class="card-text" style="font-size:15px;
                                    margin-top: 10px;">
                                        <b> Name: </b> <%=items.name%> <br> <b> Age: </b><%=items.age%> <br>  <b> Gender: </b><%=items.gender%> <br> <b> Breed: </b><%=items.breed%> <br>
                                        <!-- <b> Description: </b><%=items.description%>  -->
                                    </p>
                                    <form action="/dog/delete/<%=items._id%>" method="post">
                                        <div class="d-flex justify-content-between align-items-center">
                                            <button type="submit" class="btn btn-danger" style="background-color: orangered;">Remove <%=items.name%></button>
    
                                        </div>
                                    </form>
                                    
                                </div>
                            </div>
                        </div>
                     <% })%>


                </div>
            </div>

        </div>
  
        


        <button type="button" class="btn modalbtn" data-bs-toggle="modal" data-bs-target="#exampleModal"
            data-bs-whatever="@fat">Add animal</button>
        <div class="modal fade" id="exampleModal" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
            <div class="modal-dialog">
                <div class="modal-content">
                    <div class="modal-header">
                        <h5 class="modal-title" id="exampleModalLabel"></h5>
                        <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                    </div>
                    <div class="modal-body">
                        <form action="/dogs" method="post" enctype="multipart/form-data" autocomplete="off">
                            <div class="mb-3">
                                <label for="name" class="col-form-label">Name:</label>
                                <input type="text" class="form-control" id="name" name="name">
                            </div>
                            
                            <div class="mb-3">
                                <label for="age" class="col-form-label">Age:</label>
                                <input type="text" class="form-control" id="age" name="age">
                            </div>
                            <div class="mb-3">
                                <label for="age" class="col-form-label">Gender:</label>
                                <input type="text" class="form-control" id="age" name="gender">
                            </div><div class="mb-3">
                                <label for="age" class="col-form-label">Breed:</label>
                                <input type="text" class="form-control" id="age" name="breed">
                            </div>
                           
                            <div class="mb-3">
                                <label for="picture" class="col-form-label">Upload picture:</label>
                                <input type="file" name="image" id="photo"class="form-control">
                            </div>
                            <div class="modal-footer">
                                <button type="button" class="btn  " data-bs-dismiss="modal">Close</button>
                                <button type="submit" class="btn " >Submit</button>
                            </div>
                        </form>
                    </div>

                </div>
            </div>
        </div>

    </main>

    <div class="container">
        <footer class="d-flex flex-wrap justify-content-between align-items-center py-3 my-4 ">
            <div class="col-md-4 d-flex align-items-center">
